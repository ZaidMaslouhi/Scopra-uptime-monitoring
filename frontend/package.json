{
  "name": "scopra-frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.7",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "firebase": "^9.16.0",
    "lottie-react": "^2.3.1",
    "octokit": "^2.0.14",
    "react": "^18.2.0",
    "react-credit-cards-2": "^1.0.1",
